class s {
	String[] so = { "", "một", "hai", "ba", "bốn", "năm", "sáu", "bảy", "tám", "chín" };

	public String open(String s) {
		Integer number = Integer.parseInt(s);
		return docso(number);
	}

	public String docso(int n) {
		int count = count(n);
		if (count == 1) {
			return n == 0 ? "không" : so[n % 10];
		}
		if (count == 2) {
			if (n == 10)
				return "mười";
			if (n > 10 && n < 20)
				return "mười " + so[n % 10];
			else
				return so[n / 10 % 10] + " mươi " + so[n % 10];
		}
		if (count == 3) {
			if (n % 100 < 10 && n % 100 > 0) {
				return so[n / 100 % 10] + " trăm linh " + so[n % 10];
			}else if(n % 100 == 0) {
				return so[n / 100 % 10] + " trăm";
			}
			else {
				return so[n / 100 % 10] + " trăm " + docso(n % 100);
			}
		}
		if ( count > 3 && count <= 6) {
			if ( n %1000 == 0)
				return docso(n / 1000) + " nghìn";
			if ( n % 1000 < 10 &&  n % 1000 > 0 )
				return docso(n / 1000) + " nghìn linh " + docso(n % 1000);
			if ( (n % 1000) / 100 == 0)
				return docso(n / 1000) + " nghìn không trăm " + (n % 1000 < 10 &&  n % 1000 > 0)docso(n % 1000);
			return docso(n / 1000) + " nghìn " + docso(n % 1000);
		}
		return "";
	}

	public int count(int n) {
		int dem = 0;
		while (n >= 10) {
			n /= 10;
			dem++;
		}
		return dem + 1;
	}
}

public class Main {
	public static void main(String[] args) {
		s a = new s();
		System.out.println(a.open("1010"));
	}
}
